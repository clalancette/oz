<!-- An example that shows how to do a basic install of OpenSUSE
     11.3 i386 using the ISO type install.
-->
<template>
  <name>opensuse11.3jeos</name>
  <os>
    <name>OpenSUSE</name>
    <version>11.3</version>
    <arch>i386</arch>
    <install type='iso'>
      <iso>http://download.opensuse.org/distribution/11.3/iso/openSUSE-11.3-DVD-i586.iso</iso>
    </install>
  </os>
  <description>OpenSUSE 11.3</description>
</template>
