<!-- An example that shows how to do a basic install of Ubuntu 9.10
     i386 using the ISO type install.
-->
<template>
  <name>ubuntu9.10jeos</name>
  <os>
    <name>Ubuntu</name>
    <version>9.10</version>
    <arch>i386</arch>
    <install type='iso'>
      <iso>http://mirrors.us.kernel.org/ubuntu-releases/9.10/ubuntu-9.10-desktop-i386.iso</iso>
    </install>
  </os>
  <description>Ubuntu 9.10</description>
</template>
