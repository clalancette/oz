<!-- An example that shows how to do a basic install of Fedora 12
     i386 using the URL type install.
-->
<template>
  <name>f12jeos</name>
  <os>
    <name>Fedora</name>
    <version>12</version>
    <arch>i386</arch>
    <install type='url'>
      <url>http://download.fedoraproject.org/pub/fedora/linux/releases/12/Fedora/i386/os/</url>
    </install>
  </os>
  <description>Fedora 12</description>
</template>
