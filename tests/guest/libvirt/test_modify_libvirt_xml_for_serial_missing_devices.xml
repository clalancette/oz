<?xml version="1.0"?>
<domain type="qemu">
  <name>tester</name>
  <memory>1048576</memory>
  <currentMemory>1048576</currentMemory>
  <uuid>%s</uuid>
  <clock offset="utc"/>
  <vcpu>1</vcpu>
  <features>
    <acpi/>
    <apic/>
    <pae/>
  </features>
  <os>
    <type>hvm</type>
    <boot dev="hd"/>
  </os>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>destroy</on_reboot>
  <on_crash>destroy</on_crash>
</domain>
